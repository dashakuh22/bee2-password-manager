{
    "name": "Bee2",
    "description": "Bee2 Password Manager",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "activeTab",
        "identity"
    ],
    "content_security_policy": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'",
    "browser_action": {
	    "default_icon": {
		"19": "favicon.ico"
	    },
	    "default_title": "Bee2 Password Manager",
	    "default_popup": "index.html"
    }
}
